{
  "family": "spotter-app-task",
  "taskRoleArn": "arn:aws:iam::920013187591:role/SpotterTaskRole",
  "executionRoleArn": "arn:aws:iam::920013187591:role/SpotterTaskExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "spotter-web",
      "image": "920013187591.dkr.ecr.us-east-1.amazonaws.com/spotter-app:latest",
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "DYNAMODB_USERS_TABLE",
          "value": "spotter-users"
        },
        {
          "name": "DATABASE_URL",
          "value": "file:/tmp/prisma/prod.db"
        },
        {
          "name": "AUTH_TRUST_HOST",
          "value": "true"
        },
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "NEXTAUTH_URL",
          "value": "https://kinexfit.com"
        },
        {
          "name": "DYNAMODB_BODY_METRICS_TABLE",
          "value": "spotter-body-metrics"
        },
        {
          "name": "DYNAMODB_WORKOUTS_TABLE",
          "value": "spotter-workouts"
        },
        {
          "name": "ADMIN_EMAIL",
          "value": "alejo@kinexfit.com"
        },
        {
          "name": "EMAIL_FROM",
          "value": "alejo@kinexfit.com"
        },
        {
          "name": "UPSTASH_REDIS_REST_URL",
          "value": "https://pumped-katydid-26778.upstash.io"
        },
        {
          "name": "UPSTASH_REDIS_REST_TOKEN",
          "value": "AWiaAAIncDFhYjk5N2NlMzczZDU0OWYwYWRiMzJkNDZkMjljYTBlZHAxMjY3Nzg"
        },
        {
          "name": "AWS_REGION",
          "value": "us-east-1"
        }
      ],
      "secrets": [
        {
          "name": "AUTH_SECRET",
          "valueFrom": "arn:aws:ssm:us-east-1:920013187591:parameter/spotter-app/AUTH_SECRET"
        },
        {
          "name": "STRIPE_ALLOW_TEST_IN_PRODUCTION",
          "valueFrom": "arn:aws:ssm:us-east-1:920013187591:parameter/spotter-app/STRIPE_ALLOW_TEST_IN_PRODUCTION"
        },
        {
          "name": "GOOGLE_CLIENT_ID",
          "valueFrom": "arn:aws:ssm:us-east-1:920013187591:parameter/spotter-app/prod/GOOGLE_CLIENT_ID"
        },
        {
          "name": "GOOGLE_CLIENT_SECRET",
          "valueFrom": "arn:aws:ssm:us-east-1:920013187591:parameter/spotter-app/prod/GOOGLE_CLIENT_SECRET"
        },
        {
          "name": "FACEBOOK_CLIENT_ID",
          "valueFrom": "arn:aws:ssm:us-east-1:920013187591:parameter/spotter-app/prod/FACEBOOK_CLIENT_ID"
        },
        {
          "name": "FACEBOOK_CLIENT_SECRET",
          "valueFrom": "arn:aws:ssm:us-east-1:920013187591:parameter/spotter-app/prod/FACEBOOK_CLIENT_SECRET"
        },
        {
          "name": "EMAIL_SERVER_HOST",
          "valueFrom": "arn:aws:ssm:us-east-1:920013187591:parameter/spotter-app/prod/EMAIL_SERVER_HOST"
        },
        {
          "name": "EMAIL_SERVER_PORT",
          "valueFrom": "arn:aws:ssm:us-east-1:920013187591:parameter/spotter-app/prod/EMAIL_SERVER_PORT"
        },
        {
          "name": "EMAIL_SERVER_USER",
          "valueFrom": "arn:aws:ssm:us-east-1:920013187591:parameter/spotter-app/prod/EMAIL_SERVER_USER"
        },
        {
          "name": "EMAIL_SERVER_PASSWORD",
          "valueFrom": "arn:aws:ssm:us-east-1:920013187591:parameter/spotter-app/prod/EMAIL_SERVER_PASSWORD"
        },
        {
          "name": "EMAIL_SERVER_SECURE",
          "valueFrom": "arn:aws:ssm:us-east-1:920013187591:parameter/spotter-app/prod/EMAIL_SERVER_SECURE"
        },
        {
          "name": "APIFY_API_TOKEN",
          "valueFrom": "arn:aws:ssm:us-east-1:920013187591:parameter/spotter-app/APIFY_API_TOKEN"
        },
        {
          "name": "STRIPE_SECRET_KEY",
          "valueFrom": "arn:aws:ssm:us-east-1:920013187591:parameter/spotter-app/STRIPE_SECRET_KEY"
        },
        {
          "name": "STRIPE_WEBHOOK_SECRET",
          "valueFrom": "arn:aws:ssm:us-east-1:920013187591:parameter/spotter-app/STRIPE_WEBHOOK_SECRET"
        },
        {
          "name": "STRIPE_PUBLISHABLE_KEY",
          "valueFrom": "arn:aws:ssm:us-east-1:920013187591:parameter/spotter-app/STRIPE_PUBLISHABLE_KEY"
        },
        {
          "name": "NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY",
          "valueFrom": "arn:aws:ssm:us-east-1:920013187591:parameter/spotter-app/STRIPE_PUBLISHABLE_KEY"
        },
        {
          "name": "STRIPE_PRICE_STARTER",
          "valueFrom": "arn:aws:ssm:us-east-1:920013187591:parameter/spotter-app/STRIPE_PRICE_STARTER"
        },
        {
          "name": "STRIPE_PRICE_PRO",
          "valueFrom": "arn:aws:ssm:us-east-1:920013187591:parameter/spotter-app/STRIPE_PRICE_PRO"
        },
        {
          "name": "STRIPE_PRICE_ELITE",
          "valueFrom": "arn:aws:ssm:us-east-1:920013187591:parameter/spotter-app/STRIPE_PRICE_ELITE"
        },
        {
          "name": "NEXT_PUBLIC_STRIPE_PRICE_STARTER",
          "valueFrom": "arn:aws:ssm:us-east-1:920013187591:parameter/spotter-app/STRIPE_PRICE_STARTER"
        },
        {
          "name": "NEXT_PUBLIC_STRIPE_PRICE_PRO",
          "valueFrom": "arn:aws:ssm:us-east-1:920013187591:parameter/spotter-app/STRIPE_PRICE_PRO"
        },
        {
          "name": "NEXT_PUBLIC_STRIPE_PRICE_ELITE",
          "valueFrom": "arn:aws:ssm:us-east-1:920013187591:parameter/spotter-app/STRIPE_PRICE_ELITE"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/spotter-app",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "systemControls": []
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024"
}
